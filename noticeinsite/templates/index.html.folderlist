<br>
<br>
        <div class="tree well">
        <ul>
          {% for folder in foldersfiles %}
             <li>
             <span><i class="glyphicon glyphicon-folder-open"></i> {{ folder }}</span>
             <ul>
             {% for file in foldersfiles[folder] %}
                 <li>
                   <span><i class="glyphicon glyphicon-file"></i></span> <a href="{{ file }}">{{ file | basename }}</a>
                 </li>

             {% endfor %}
           </ul>
             </li>
          {% endfor %}
        </ul>
        </div>
